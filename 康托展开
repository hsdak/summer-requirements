#include<iostream>
#include<cstdio>
#include<cmath>
using namespace std;
int A(int a){//求阶层函数 
	int i,n=1;
	for(i=1;i<=a;i++){
		n*=i;
	}
	return n; 
}
int main(){
	int a[9],num,i,j,x=0,count;//a[]储存被展开数的每一位 
	cin>>num;//输入被展开数 
	for(i=0;;i++){//for循环将num储存在a[]中，循环结束时i的值为位数减一 
		a[i]=num%10;
		if(num/10==0){
			break;
		}
		num/=10;
	}
	for(;i>=0;i--){//计算康托展开 
		count=0;
		for(j=i;j>=0;j--){
			if(a[j]<a[i]){
				count++;
			}
		}
		x+=count*A(i);//逐项累加 
	}
	cout<<"x="<<x+1<<endl;//输出排位 
}
